<Page
    x:Class="AniConApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AniConApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="Black">
        <!--{ThemeResource ApplicationPageBackgroundThemeBrush}">-->
        <Image x:Name="image" HorizontalAlignment="Left" Height="224" Margin="74,110,0,0" VerticalAlignment="Top" Width="238" Source="ms-appx:///Assets/logo.png"/>
        <Button x:Name="button" Content="Go to AniCons" HorizontalAlignment="Left" Margin="74,339,0,0" VerticalAlignment="Top" Height="84" Width="205" Foreground="White" Background="#FF571A1A" Click="gotoAnicons"/>

    </Grid>
</Page>
